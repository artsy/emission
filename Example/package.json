  "license": "MIT",
  "metadata": {
    "graphql": {
      "schema": "./node_modules/emission/data/schema.json"
    }
  },
  "devDependencies": {
    "babel-relay-plugin": "^0.8.1",
    "babel-relay-plugin-loader": "^0.8.1"
  }
}
