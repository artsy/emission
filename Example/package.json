{
  "name": "Emission-Example",
  "version": "0.0.1",
  "description": "Emission Example App",
  "main": "Emission/index.ios.js",
  "dependencies": {
    "emission": "file:../"
  },
  "scripts": {
    "start": "cd node_modules/react-native && npm start --",
    "clean": "watchman watch-del . && npm run clean-xcode && npm run clean-module",
    "clean-xcode": "xcodebuild -workspace Emission.xcworkspace -scheme Emission -destination 'platform=iOS Simulator,name=iPhone 6' clean",
    "clean-module": "rm -rf ./node_modules/emission"
  },
